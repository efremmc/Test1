    &lt;math> 
      &lt;mrow> 
        &lt;mo>&amp;int;&lt;/mo> 
        &lt;mi>x&lt;/mi> 
        &lt;mo>=&lt;/mo> 
        &lt;mo largeop='true'>&amp;int;&lt;/mo> 
        &lt;mi>x&lt;/mi> 
        &lt;mo>=&lt;/mo> 
        &lt;mo largeop='false'>&amp;int;&lt;/mo> 
        &lt;mi>x&lt;/mi> 
        &lt;mo>=&lt;/mo> 
        &lt;mstyle displaystyle='true'> 
          &lt;mo>&amp;int;&lt;/mo> 
          &lt;mi>x&lt;/mi> 
          &lt;mo>=&lt;/mo> 
          &lt;mo largeop='true'>&amp;int;&lt;/mo> 
          &lt;mi>x&lt;/mi> 
          &lt;mo>=&lt;/mo> 
          &lt;mo largeop='false'>&amp;int;&lt;/mo> 
          &lt;mi>x&lt;/mi> 
        &lt;/mstyle> 
      &lt;/mrow> 
    &lt;/math>
