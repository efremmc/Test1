    <p>
      <b>Source Code</b>:</p>
    <pre>
    &lt;math> 
      &lt;mrow> 
        &lt;munderover> 
          &lt;mo>&amp;sum;&lt;/mo> 
          &lt;mi>x&lt;/mi> 
          &lt;mn>0&lt;/mn> 
        &lt;/munderover> 
        &lt;mo>=&lt;/mo> 
        &lt;munderover> 
          &lt;mo movablelimits='true'>&amp;sum;&lt;/mo> 
          &lt;mi>x&lt;/mi> 
          &lt;mn>0&lt;/mn> 
        &lt;/munderover> 
        &lt;mo>=&lt;/mo> 
        &lt;munderover> 
          &lt;mo movablelimits='false'>&amp;sum;&lt;/mo> 
          &lt;mi>x&lt;/mi> 
          &lt;mn>0&lt;/mn> 
        &lt;/munderover> 
        &lt;mo>=&lt;/mo> 
        &lt;mstyle displaystyle='true'> 
          &lt;munderover> 
            &lt;mo>&amp;sum;&lt;/mo> 
            &lt;mi>x&lt;/mi> 
            &lt;mn>0&lt;/mn> 
          &lt;/munderover> 
          &lt;mo>=&lt;/mo> 
          &lt;munderover> 
            &lt;mo movablelimits='true'>&amp;sum;&lt;/mo> 
            &lt;mi>x&lt;/mi> 
            &lt;mn>0&lt;/mn> 
          &lt;/munderover> 
          &lt;mo>=&lt;/mo> 
          &lt;munderover> 
            &lt;mo movablelimits='false'>&amp;sum;&lt;/mo> 
            &lt;mi>x&lt;/mi> 
            &lt;mn>0&lt;/mn> 
          &lt;/munderover> 
        &lt;/mstyle> 
      &lt;/mrow> 
    &lt;/math>
  </pre>
