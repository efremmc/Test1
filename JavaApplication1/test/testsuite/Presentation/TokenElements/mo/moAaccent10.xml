    <p>
      <b>Source Code</b>:</p>
    <pre>
    &lt;math> 
      &lt;mrow> 
        &lt;mover> 
          &lt;mrow> 
            &lt;mi>G&lt;/mi> 
            &lt;mo>-&lt;/mo> 
            &lt;mn>1&lt;/mn> 
          &lt;/mrow> 
          &lt;mo accent='false'>&amp;OverBar;&lt;/mo> 
        &lt;/mover> 
        &lt;mo accent='false'>+&lt;/mo> 
        &lt;mover accent='true'> 
          &lt;mrow> 
            &lt;mi>G&lt;/mi> 
            &lt;mo>-&lt;/mo> 
            &lt;mn>1&lt;/mn> 
          &lt;/mrow> 
          &lt;mo accent='false'>&amp;OverBar;&lt;/mo> 
        &lt;/mover> 
        &lt;mo accent='true'>+&lt;/mo> 
        &lt;mover accent='false'> 
          &lt;mrow> 
            &lt;mi>G&lt;/mi> 
            &lt;mo>-&lt;/mo> 
            &lt;mn>1&lt;/mn> 
          &lt;/mrow> 
          &lt;mo accent='false'>&amp;OverBar;&lt;/mo> 
        &lt;/mover> 
      &lt;/mrow> 
    &lt;/math>
  </pre>
